CONFIGURATION_BUILD_DIR = ${PODS_CONFIGURATION_BUILD_DIR}/GLPlayer
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Private" "${PODS_ROOT}/Headers/Public"
LIBRARY_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/../../GLPlayer/Classes/libamr/libmp3lame/lib" "${PODS_ROOT}/../../GLPlayer/Classes/libamr/opencore-amrnb" "${PODS_ROOT}/../../GLPlayer/Classes/libamr/opencore-amrwb"
OTHER_LDFLAGS = -l"mp3lame" -l"opencore-amrnb" -l"opencore-amrwb"
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}
PODS_TARGET_SRCROOT = ${PODS_ROOT}/../..
PRODUCT_BUNDLE_IDENTIFIER = org.cocoapods.${PRODUCT_NAME:rfc1034identifier}
SKIP_INSTALL = YES
